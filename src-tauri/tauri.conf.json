{
  "build": {
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "path-algorithms",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true,
        "ask": true,
        "confirm": true,
        "message": true,
        "open": true,
        "save": true
      },
      "updater": {
        "active": true,
        "endpoints": [
          "https://github.com/FarerABR/path-algorithms/releases/{{target}}/{{arch}}/{{current_version}}"
        ],
        "dialog": true,
        "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDE3ODRBNTVCNDFGRDQ4NEIKUldSTFNQMUJXNldFRjBKd0ljY01GQ3J2ZDZiTkZVOUlMejFsQVV4WjNTSTl0bC9OTTdwTnBVVFoK"
      },
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "path-algorithms",
        "width": 1200,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "me.farerabr.path",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}